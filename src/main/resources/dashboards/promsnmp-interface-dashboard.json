        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "rate(ifHCInOctets{instance=~\"$instance\", ifName=~\"$interface\"}[$__rate_interval]) * 8",
          "legendFormat": "In - {{ifName}}",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "rate(ifHCOutOctets{instance=~\"$instance\", ifName=~\"$interface\"}[$__rate_interval]) * -8",
          "legendFormat": "Out - {{ifName}}",
          "range": true,
          "refId": "B"
        }
      ],
      "title": "Throughput (In/Out)",
      "type": "timeseries"
